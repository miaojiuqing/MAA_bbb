{
    "name": "MAA_bbb",
    "url": "https://github.com/miaojiuqing/MAA_bbb",
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        },
        {
            "name": "B 服",
            "path": [
                "{PROJECT_DIR}/resource",
                "{PROJECT_DIR}/resource_bilibili"
            ]
        }
    ],
    "agent_demo": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/agent/main.py"
        ]
    },
    "task": [
        {
            "name": "崩坏三 启动！",
            "entry": "启动并进入游戏",
            "option": [
                "登录渠道"
            ],
            "doc": "会自行确认/领取所有奖励"
        },
        {
            "name": "材料远征",
            "entry": "材料远征-开始任务层"
        },
        {
            "name": "日常奖励领取",
            "entry": "任务奖励领取-任务开始层"
        },
        {
            "name": "参数任务",
            "entry": "MyTask3",
            "pipeline_override": {}
        },
        {
            "name": "带Custom的任务",
            "entry": "MyTask4"
        }
    ],
    "option": {
        "登录渠道": {
            "cases": [
                {
                    "name": "官服",
                    "pipeline_override": {
                        "启动并进入游戏": {
                            "action": "StartApp",
                            "package": "com.miHoYo.enterprise.NGHSoD"
                        }
                    }
                },
                {
                    "name": "B服",
                    "pipeline_override": {
                        "启动并进入游戏": {
                            "action": "StartApp",
                            "package": "com.miHoYo.bh3.bilibili"
                        }
                    }
                }
            ],
            "default_case": "官服"
        }
    }
}