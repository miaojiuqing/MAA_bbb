{
    "name": "MAA_bbb",
    "url": "https://github.com/miaojiuqing/MAA_bbb",
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        },
        {
            "name": "桌面端",
            "path": [
                "{PROJECT_DIR}/resource",
                "{PROJECT_DIR}/resource_win32"
            ]
        }
    ],
    "agent_demo": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/agent/main.py"
        ]
    },
    "task": [
        {
            "name": "崩坏三 启动！",
            "entry": "启动并进入游戏",
            "option": [
                "登录渠道"
            ],
            "doc": "左上角可选模拟器或者桌面端，桌面端需自行选择游戏窗口\n\n会自行确认/领取所有奖励"
        },
        {
            "name": "材料远征",
            "entry": "材料远征-开始任务层"
        },
        {
            "name": "日常奖励领取",
            "entry": "任务奖励领取-任务开始层"
        },
        {
            "name": "凭证奖励尝试领取",
            "entry": "凭证奖励领取-任务开始层",
            "doc": "不知道该怎么判断有没有奖励，干脆每次进去点一下，有就有没有就没有"
        },
        {
            "name": "家园资源领取",
            "entry": "进入家园领取金币/体力-开始任务层",
            "pipeline_override": {},
            "doc": "只领金币"
        },
        {
            "name": "家园远征派遣",
            "entry": "家园远征-开始任务层",
            "option": [
                "派遣时体力不足吃药吗"
            ]
        },
        {
            "name": "家园打工",
            "entry": "家园打工-开始任务层",
            "doc": "未收录水晶奖励/无法上的班弹窗情况，遇到请F9截屏后提issue"
        },
        {
            "name": "领取邮箱奖励",
            "entry": "邮箱-任务开始层"
        },
        {
            "name": "舰团每日委托",
            "entry": "舰团每日委托-开始任务层"
        }
    ],
    "option": {
        "登录渠道": {
            "cases": [
                {
                    "name": "官服",
                    "pipeline_override": {
                        "启动并进入游戏": {
                            "action": "StartApp",
                            "package": "com.miHoYo.enterprise.NGHSoD"
                        }
                    }
                },
                {
                    "name": "B服",
                    "pipeline_override": {
                        "启动并进入游戏": {
                            "action": "StartApp",
                            "package": "com.miHoYo.bh3.bilibili"
                        }
                    }
                }
            ],
            "default_case": "官服"
        },
        "派遣时体力不足吃药吗": {
            "cases": [
                {
                    "name": "YES",
                    "pipeline_override": {
                        "家园远征-体力不足提示": {
                            "next": [
                                "启动并进入游戏"
                            ]
                        }
                    }
                },
                {
                    "name": "NO",
                    "pipeline_override": {
                        "家园远征-体力不足提示": {
                            "next": [
                                "启动并进入游戏"
                            ]
                        }
                    }
                }
            ],
            "default_case": "NO"
        }
    }
}